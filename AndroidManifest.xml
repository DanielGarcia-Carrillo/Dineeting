<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thedineeting"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.thedineeting.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.thedineeting.Homepage"
            android:label="@string/title_activity_homepage" >
        </activity>
        <activity
            android:name="com.example.thedineeting.EventsStatus"
            android:label="@string/title_activity_events_status" >
        </activity>
        <activity
            android:name="com.example.thedineeting.Restaurant"
            android:label="@string/title_activity_restaurant" >
        </activity>
        <activity
            android:name="com.example.thedineeting.MessagesActivity"
            android:label="@string/title_activity_messages" >
        </activity>
        <activity
            android:name="com.example.thedineeting.Notifications"
            android:label="@string/title_activity_notifications" >
        </activity>
        <activity
            android:name="com.example.thedineeting.Friends"
            android:label="@string/title_activity_friends" >
        </activity>
        <activity
            android:name="com.example.thedineeting.DateTime"
            android:label="@string/title_activity_date_time" >
        </activity>
        <activity
            android:name="com.example.thedineeting.Voting"
            android:label="@string/title_activity_voting" >
        </activity>
        <activity
            android:name="com.example.thedineeting.Messagess"
            android:label="@string/title_activity_messagess" >
        </activity>
    </application>

</manifest>
